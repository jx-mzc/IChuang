<view style='margin-top:20rpx'>
  <view class="showback">
    <view class="font">你的练习成绩</view>
  </view>
  <view class="showgrade">
    <text class="fontgrade">{{grade}}</text>
  </view>
</view>
<view style='margin-top:20rpx'>
  <view class="showback">
    <view class="font">你的答案</view>
  </view>
  <view class="showgrade" >
  <block wx:for="{{choose}}" >
    <text style='margin-left:20rpx'>{{item}}</text>
    </block>
  </view>
</view>
<view style='margin-top:20rpx'>
  <view class="showback">
    <view class="font">标准答案</view>
  </view>
   <view class="showgrade" >
  <block wx:for="{{finalanswer}}">
    <text style='margin-left:20rpx'>{{item}}</text>
    </block>
  </view>

</view>
<view style='margin-top:20rpx'>

   <view class="showgrade" style='  border-top-right-radius: 20px;border-top-left-radius: 20px;'>
    <text style='margin-left:20rpx'>于<text style='font-size: 40rpx;color: red;'>{{time}}</text>获得<text style='font-size: 40rpx;color: red;'>{{grade}}</text>个积分</text>
  </view>

</view>