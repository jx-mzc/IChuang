
<view class="photo">
  <text class="font1">{{name}}</text> 
</view> 

<view class="boom">
  <text class="font">活动开始时间</text>
  <text class="textr">{{start_time}}</text>
</view>
<view class="boom">
  <text class="font">活动结束时间</text>
  <text class="textr">{{end_time}}</text>
</view>
<view class="boom">
  <text class="font">活动发布方</text>
  <text class="textr">{{post}}</text>
</view>
<view class="boom">
  <text class="font">活动举办方</text>
  <text class="textr">{{juban}}</text>
</view>

<view class="photoboom">
  <view class = "textboom" bindtap='showfile'>查看活动文档</view>
  <view class = "textboom">报名</view>
</view>
