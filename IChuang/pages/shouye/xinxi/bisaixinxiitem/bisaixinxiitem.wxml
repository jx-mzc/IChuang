
<view class="photo">
  <text class="font1">{{name}}</text> 
</view> 

<view class="boom">
  <text class="font">比赛开始时间</text>
  <text class="textr">{{start_time}}</text>
</view>
<view class="boom">
  <text class="font">比赛结束时间</text>
  <text class="textr">{{end_time}}</text>
</view>
<view class="boom">
  <text class="font">比赛发布方</text>
  <text class="textr">{{post}}</text>
</view>
<view class="boom">
  <text class="font">比赛举办方</text>
  <text class="textr">{{juban}}</text>
</view>
<view class="boom">
  <text class="font">大赛级别</text>
  <text class="textr">{{rank}}</text>
</view>

<view class="photoboom">
  <view class = "textboom" bindtap='showfile'>查看比赛文档</view>
  <!-- <view class = "textboom">报名</view> -->
</view>
